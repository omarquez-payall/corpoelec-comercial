<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="invoice_images_view_form_inherit">
            <field name="name">account.move.form.inherit</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form" />
            <field name="arch" type="xml">
                <notebook>
                    <page string="Images">
                        <field name="images" />
                        <group><field name="images.logo" widget="image" class="oe_avatar"/></group>
                        <group><field name="images.header_logo" widget="image" class="oe_avatar"/></group>
                        <group><field name="images.full_logo" widget="image" class="oe_avatar"/></group>
                        <group><field name="images.payment_methods" widget="image" class="oe_avatar"/></group>
                    </page> 
                </notebook>
            </field>
        </record>
    </data>
</odoo>